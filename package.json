    "react-router-dom": "^7.9.5",
    "shell-quote": "^1.8.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "scripts": {
    "dev": "concurrently \"bun run dev:server\" \"bun run dev:client\"",
    "dev:client": "vite",
    "dev:server": "bun run --watch src/server/index.ts",
    "build": "vite build && bun build src/server/index.ts --outdir dist/server --target bun",
    "start": "bun run dist/server/index.js",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.client.json && tsc --noEmit -p tsconfig.server.json"
  }
}
